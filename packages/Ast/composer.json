{
    "name": "railt/ast",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "GraphQL abstract syntax tree",
    "keywords": [
        "ast",
        "nodes"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/ast"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "phplrt/ast-contracts": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Ast\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Ast\\Tests\\Unit\\": "tests/unit",
            "Railt\\Ast\\Tests\\Feature\\": "tests/feature"
        }
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "vimeo/psalm": "^3.5",
        "phpunit/phpunit": "^8.3"
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
