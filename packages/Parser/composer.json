{
    "name": "railt/parser",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The GraphQL parser",
    "keywords": [
        "graphql",
        "sdl",
        "parser"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/parser"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "railt/ast": "^2.0",
        "phplrt/lexer": "~2.2",
        "phplrt/parser": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Parser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Parser\\Tests\\Unit\\": "tests/unit",
            "Railt\\Parser\\Tests\\Feature\\": "tests/feature"
        }
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "vimeo/psalm": "^3.5",
        "phpunit/phpunit": "^8.3",
        "twig/twig": "~2.11",
        "phplrt/compiler": "~2.2",
        "symfony/console": "~3.4|~4.0|~5.0",
        "zendframework/zend-code": "~3.3",
        "ocramius/package-versions": "~1.5"
    },
    "extra": {
        "railt": {
            "commands": [
                "Railt\\Parser\\Console\\GrammarCompileCommand"
            ]
        },
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
