{
    "name": "railt/extension",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework extensions package",
    "keywords": [
        "extensions",
        "extension",
        "extend",
        "provider",
        "service",
        "loader"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/extension"
    },
    "require": {
        "php": ">=7.4",
        "railt/container": "^2.0",
        "ramsey/collection": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Extension\\": "src"
        }
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "vimeo/psalm": "^3.5",
        "phpunit/phpunit": "^8.3",
        "railt/config": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Extension\\Tests\\Unit\\": "tests/unit",
            "Railt\\Extension\\Tests\\Feature\\": "tests/feature"
        }
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
