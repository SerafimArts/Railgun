{
    "name": "railt/type-system",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The Railt Framework implementation of GraphQL Type System",
    "keywords": [
        "graphql",
        "type system",
        "sdl",
        "dto",
        "types"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/type-system"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "graphql/type-system-contracts": "~14.0|dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Railt\\TypeSystem\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\TypeSystem\\Tests\\Unit\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "@php vendor/phpunit/phpunit/phpunit"
    },
    "extra": {
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
