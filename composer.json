{
    "name": "railt/railt",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "type": "library",
    "description": "The Railt Framework",
    "keywords": ["graphql", "sdl", "ast", "framework", "reflection", "railt", "application"],
    "homepage": "http://railt.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "bin": [
        "bin/railt"
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "composer-plugin-api": "^1.1",
        "justinrainbow/json-schema": "~5.2",
        "ocramius/package-versions": "~1.5",
        "phplrt/ast-contracts": "~2.2",
        "phplrt/lexer": "~2.2",
        "phplrt/parser": "~2.2",
        "phplrt/position": "~2.2",
        "phplrt/source": "~2.2",
        "phplrt/visitor": "~2.2",
        "psr/container": "~1.0",
        "ramsey/collection": "~1.0"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "illuminate/container": "~5.5|~6.0",
        "illuminate/http": "~5.5|~6.0",
        "ocramius/package-versions": "~1.5",
        "phplrt/compiler": "~2.2",
        "phpunit/phpunit": "^8.3",
        "symfony/console": "~3.4|~4.0|~5.0",
        "symfony/dependency-injection": "~3.4|~4.0|~5.0",
        "symfony/http-foundation": "~3.4|~4.0|~5.0",
        "symfony/process": "~3.4|~4.0|~5.0",
        "symfony/var-dumper": "~3.4|~4.0|~5.0",
        "symplify/monorepo-builder": "^6.1",
        "twig/twig": "~2.11",
        "vimeo/psalm": "^3.5",
        "zendframework/zend-code": "~3.3",
        "zendframework/zend-diactoros": "~1.7|~2.0",
        "zendframework/zend-http": "~2.8"
    },
    "autoload": {
        "files": [
            "packages/Dumper/src/helpers.php"
        ],
        "psr-4": {
            "Railt\\Ast\\": "packages/Ast/src",
            "Railt\\Config\\": "packages/Config/src",
            "Railt\\Container\\": "packages/Container/src",
            "Railt\\Discovery\\": "packages/Discovery/src",
            "Railt\\Dumper\\": "packages/Dumper/src",
            "Railt\\Foundation\\": "src",
            "Railt\\Http\\": "packages/Http/src/",
            "Railt\\Parser\\": "packages/Parser/src",
            "Railt\\Pipeline\\": "packages/Pipeline/src/",
            "Railt\\TypeSystem\\": "packages/TypeSystem/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Ast\\Tests\\Feature\\": "packages/Ast/tests/feature",
            "Railt\\Ast\\Tests\\Unit\\": "packages/Ast/tests/unit",
            "Railt\\Config\\Tests\\Feature\\": "packages/Config/tests/feature",
            "Railt\\Config\\Tests\\Unit\\": "packages/Config/tests/unit",
            "Railt\\Container\\Tests\\Feature\\": "packages/Container/tests/feature",
            "Railt\\Container\\Tests\\Unit\\": "packages/Container/tests/unit",
            "Railt\\Discovery\\Tests\\Feature\\": "packages/Discovery/tests/feature",
            "Railt\\Discovery\\Tests\\Unit\\": "packages/Discovery/tests/unit",
            "Railt\\Dumper\\Tests\\Feature\\": "packages/Dumper/tests/feature",
            "Railt\\Dumper\\Tests\\Unit\\": "packages/Dumper/tests/unit",
            "Railt\\Http\\Tests\\Feature\\": "packages/Http/tests/feature",
            "Railt\\Http\\Tests\\Unit\\": "packages/Http/tests/unit",
            "Railt\\Parser\\Tests\\Feature\\": "packages/Parser/tests/feature",
            "Railt\\Parser\\Tests\\Unit\\": "packages/Parser/tests/unit",
            "Railt\\Pipeline\\Tests\\Feature\\": "packages/Pipeline/tests/feature",
            "Railt\\Pipeline\\Tests\\Unit\\": "packages/Pipeline/tests/unit",
            "Railt\\TypeSystem\\Tests\\Feature\\": "packages/TypeSystem/tests/feature",
            "Railt\\TypeSystem\\Tests\\Unit\\": "packages/TypeSystem/tests/unit"
        }
    },
    "replace": {
        "railt/config": "self.version",
        "railt/container": "self.version",
        "railt/discovery": "self.version",
        "railt/dumper": "self.version",
        "railt/extension": "self.version",
        "railt/http": "self.version",
        "railt/parser": "self.version",
        "railt/sdl": "self.version",
        "railt/ast": "self.version",
        "railt/exception": "self.version",
        "railt/pipeline": "self.version"
    },
    "provide": {
        "psr/container": "1.0",
        "psr/container-implementation": "1.0"
    },
    "scripts": {
        "post-autoload-dump": "Railt\\Discovery\\Manifest::discover"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "discovery": {
            "discovery": "${package.dir}/packages/Discovery/resources/discovery.schema.json",
            "railt": "${package.dir}/resources/railt.schema.json"
        },
        "branch-alias": {
            "1.3.x": "1.3.x-dev",
            "1.4.x": "1.4.x-dev",
            "dev-master": "2.0.x-dev"
        },
        "class": "Railt\\Discovery\\Manifest",
        "railt": {
            "commands": [
                "Railt\\Foundation\\Console\\Command\\RepoSyncCommand",
                "Railt\\Foundation\\Console\\Command\\RepoMergeCommand",
                "Railt\\Foundation\\Console\\Command\\ExtensionsListCommand",
                "Railt\\Parser\\Console\\ParserCompileCommand"
            ]
        }
    }
}
