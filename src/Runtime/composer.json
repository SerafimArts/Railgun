    "keywords": ["language", "ll1", "llk", "context-free", "parser", "pp", "ast", "syntax"],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Ivan Enderlin",
            "email": "ivan.enderlin@hoa-project.net"
        },
        {
            "name": "Hoa community",
            "homepage": "https://hoa-project.net/"
        },
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "railt/io": "1.2.*|dev-master",
        "railt/lexer": "1.2.*|dev-master",
        "hoa/iterator": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Runtime\\": "."
